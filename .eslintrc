{
  "rules": {
    "max-len": [
      "error",
      {
        "code": 125,
        "comments": 125
      }
    ],
    "no-console": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "jsx-a11y/label-has-for": 0
  },
  "extends": ["airbnb", "prettier"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["prettier"]
}
